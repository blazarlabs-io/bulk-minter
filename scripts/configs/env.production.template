# Production Environment Variables
# Copy this to .env.local on your server and fill in the actual values

# Tokenization API Configuration
TOKENIZATION_API_URL=https://your-tokenization-api.com
TOKENIZATION_API_USERNAME=your_username
TOKENIZATION_API_PASSWORD=your_password

# IPFS Configuration
IPFS_GATEWAY=https://your-ipfs-gateway.com

# Blockfrost API (Cardano blockchain)
BLOCKFROST_API_KEY=your_blockfrost_api_key

# IoT Storage API
IOT_STORAGE_SENSORS_API_URL=https://your-iot-api.com

# Next.js Configuration
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
