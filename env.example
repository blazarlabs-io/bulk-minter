# Copy this file to .env.local and fill in your actual values

# Tokenization API Configuration
TOKENIZATION_API_URL=https://your-api-url.com
TOKENIZATION_API_USERNAME=your_username
TOKENIZATION_API_PASSWORD=your_password

# IPFS Gateway
IPFS_GATEWAY=https://ipfs.io/ipfs

# Other Configuration (optional)
IOT_STORAGE_SENSORS_API_URL=https://your-iot-api.com
BLOCKFROST_API_KEY=your_blockfrost_api_key

# Force Pure JavaScript Implementations (Fix Vercel Build Issues)
CHACHA_NATIVE_DISABLE=true
CRYPTO_NATIVE_DISABLE=true
NODE_OPTIONS=--no-experimental-fetch

# Note: After creating .env.local, restart your development server
